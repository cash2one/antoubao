<script>
	var CID = "endtime";
	if(window.CID != null){
		var iTime = document.getElementById(CID).innerHTML;
		var Account;

		
	}
	function RemainTime(){
	var iDay,iHour,iMinute,iSecond;
	var sDay="",sTime="";
		if (iTime >= 0){
			iDay = parseInt(iTime/24/3600);
			iHour = parseInt((iTime/3600)%24);
			iMinute = parseInt((iTime/60)%60);
			iSecond = parseInt(iTime%60);
	  
	   if (iDay > 0){ 
		sDay = iDay + "天"; 
	   }
	   sTime =sDay + iHour + "小时" + iMinute + "分钟" + iSecond + "秒";
	  
			if(iTime==0){
			   clearTimeout(Account);
			   sTime="<span style='color:green'>时间到了！</span>";
			}else{
			   Account = setTimeout("RemainTime()",1000);
			}
			iTime=iTime-1;
		}else{
			sTime="<span style='color:red'>等待预审</span>";
		}
	document.getElementById(CID).innerHTML = sTime;
	}
	function autoInput(){

		var most_account = 0;
		var other_account = 0;
		var use_money = 0;	
		var theCount,Count;
		
		if(most_account!=0){
			if(most_account>other_account){
				theCount = 	other_account;
			}else{
				theCount = 	most_account;
			}
			if(theCount>use_money){
				Count = use_money;
			}else{
				Count = theCount;	
			}
		}else{
			if(other_account>use_money){
					Count = use_money;
			}else{
					Count = other_account;
			}
		}
		if (Count < 1) Count=1;
		
		$("#tender-money").val(parseFloat(Count));
		$("#paypassword").show(200);
							   
	}
	$(function(){
		$(".data-item:gt(4)").hide();
		$(".tender-item:gt(14)").hide();
		if($(".tender-item").size()>15){
			$("#show-tender").show();	
		}
		$("#show-tender").click(function(){
			$(".tender-item,#hide-tender").show(200);
			$("#show-tender").hide();
		})
		$("#hide-tender").click(function(){
			$(".tender-item:gt(14),#hide-tender").hide(200);
			$("#show-tender").show();
		})
		$("#show-data").click(function(){
			$(".data-item,#hide-data").show();
			$("#show-data").hide();
		})
		$("#hide-data").click(function(){
			$(".data-item:gt(4),#hide-data").hide();
			$("#show-data").show();
		})
		$("#tender-money").focus(function(){
			$("#paypassword").show(200);
		})
		$("#tender-money").blur(function(){
			if($("#tender-money").val()==''){
				$("#paypassword").hide(200);
			}
		})
		$("#truncate-txt-btn").click(function(){
			$("#all-txt").show();
			$("#truncate-txt").hide();
		})
		$("#all-txt-btn").click(function(){
			$("#all-txt").hide();
			$("#truncate-txt").show();
		})
	})
</script>

