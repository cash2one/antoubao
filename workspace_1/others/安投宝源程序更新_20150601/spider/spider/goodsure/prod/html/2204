<script type="text/javascript">
var user_id = "";

function getSD(){
	var url = '/core/sdjz.php?up=up&user_id='+user_id;//注意 定义 user_id
	$.ajax({
		type: 'GET',
		url: url,
		dataType:'json',
		cache:false,
		success:function(data){
			if(data.remark == 'L'){
				//登录
				$('.christmasJizi-font').text(data.txt);
				$('.christmasJizi').show();
			}
			if(data.remark == 'R'){
				//充值
				$('.christmasJizi-font').text(data.txt);
				$('.christmasJizi').show();
			}
			if(data.remark == 'F'){
				//发帖
				$('.christmasJizi-font').text(data.txt);
				$('.christmasJizi').show();
			}
			if(data.remark == 'H'){
				//回帖
				$('.christmasJizi-font').text(data.txt);
				$('.christmasJizi').show();
			}
			if(data.remark == 'S'){
				//体验金
				$('.christmasJizi-font').html(data.txt+"<br/><em class='christmas-ty'>体验金</em>");
				$('.christmasJizi').show();
			}
			if(data.rank>0 && data.remark == 'L'){
				//元旦送体验金
				$('#christmasJiziList1').css('background','url(/static/images/christmas/christmas-jizi2.png)');
				if(data.rank<51){
					$('.christmasJizi-paiming').html("您是2015年第 <em class='f30'>"+data.rank+"</em> 位登录<br/>获得2015体验金");
					$('.christmasJizi').show();
				}else{
					$('.christmasJizi-paiming').html("<em class='lh80'>您是2015年第 <em class='f30'>"+data.rank+"</em> 位登录</em>");
					$('.christmasJizi').show();
				}
			}
		}	
	});
}
</script>
<script type="text/javascript">
$(function(){
	//checking for css 3d transformation support
	$.support.css3d = supportsCSS3D();
	var christmasJiziBox = $("#christmasJiziBox");
	$('.christmasJiziBox-close').click(function(){

		$('.christmasJizi').hide();
		
	});
	//Listening for clicks on the ribbon links
	$(".flipLink").click(function(e){
		//flipping the forms
		christmasJiziBox.toggleClass("flipped");
		//if there is no css3 3d support, simply hide the christmasJiziList1 form (exposing the christmasJiziList2 one)
		if(!$.support.css3d){
			$("#christmasJiziList1").toggle();
		}
		e.preventDefault();
	});			
	//a helper function that checks for the support of the 3d css3 transformations
	function supportsCSS3D(){
		var props = ["perspectiveProperty","WebkitPerspective","MozPerspective"],testDom = document.createElement("a");
		for(var i = 0; i < props.length; i++){
			if(props[i] in testDom.style){
				return true;
			}
		}
		return false;
	}
});
</script> 

