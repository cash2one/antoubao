function addquestion()
{
	var username = $.trim($('#username').val());
	var user_contents = $.trim($('#user_contents').val());
	var jiekuanming = $('.jiekuanming').val();
	var type_id = $('#currentborrowid').val();

	if(!check_question_name()) return false;
	if(!check_question_content()) return false;

	$.jBox.tip('正在提交...',"loading");
	$.ajax({
		url  : cpurl+'/addcontent',
		data : {'username':username,'user_contents':user_contents,'type_id':type_id,'jiekuanming':jiekuanming},
		type : 'post',
		dataType:'json',
		success : function(d){
			if(d.status == 1)
			{
				$.jBox.tip(d.message,"success");
				$('#username').val('');
				$('#user_contents').val('');
				$("#questions").html(d.html);
			}else{
				$.jBox.tip(d.message,"fail");
			}
		}
	});
}


function check_question_name()
{
	var username = $.trim($('#username').val());
	if(username == '')
	{
        $('#username').addClass('nameError');
		$(".name-ts").html('<b class="step4spr">感叹号</b>请填写昵称！');//<b class="step4right">对号</b>		return false;
	}else{
		$(".name-ts").html('<b class="step4right">对号</b>');
		$('#username').removeClass('nameError');
		return true;
	}
}
function check_question_content()
{
	var user_contents = $.trim($('#user_contents').val());
	if(user_contents == '' || user_contents.length < 10)
	{
        $('#user_contents').addClass('textareaError');
		$(".content-ts").html('<b class="step4spr">感叹号</b>内容为必填，不得少于十个字！');//<b class="step4spr">感叹号</b>内容为必填，不得少于十个字
		return false;
	}else{
        $('#user_contents').removeClass('textareaError');
		$(".content-ts").html('<b class="step4right">对号</b>');
		return true;
	}
}

