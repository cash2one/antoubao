		$("#invest_title").html('PCCB投资确认');
		$("#invest_notice_bg").attr('class','reveal-green reveal-yellow clearfix');
		$("#invest_notice").html("您好，购买本产品的最低限额为："+base+" "+unit+"元");
		$(".reveal-list").hide();
		$(".reveal-border").hide();
		$("#invest_back").show();
		$("#invest_sure").hide();
		$('#myModal').reveal();
        return false;
    }
    $("#invest_back").hide();
	$("#invest_sure").show();
	$(".reveal-list").show();
	$(".reveal-border").show();
	ajax_list[ajax_list.length] = $.ajax({
        url: cpurl+"/show_tender_detail",
        type: "post",
        dataType: "json",
        data: { bid:bid,unit:unit,money:money },
        success: function(d) {
			if(d.status == 0) return false;
			$(".reveal-table").html(d.html);
			$("#invest_title").html('回款计划');
			$("#invest_back").show();
			$("#invest_sure").hide();
			$(".reveal-con1").hide();
			$(".reveal-con2").show();
			$("#myModal").reveal();

			/*回款计划隔行换色 */
			$(".reveal-table table").find("tr:odd").css("background-color", "#d5ffd5");
			$(".reveal-table table").find("tr:even").css("background-color", "#eeffee");
			$(".reveal-table table").find("tr:first").css("background-color", "#2a7ad2");
		}
	});
}
function del_money(obj,val){
	var money_box = $('#'+obj);
	var money = money_box.val();
	money = parseInt(money)-parseInt(val);
	if(money>0){
		money_box.val(money);
	}

}
function add_money(obj,val,rest){
	var money_box = $('#'+obj);
	var money = money_box.val();
	money = parseInt(money)+parseInt(val);
	if(money<=rest){
		money_box.val(money);
	}
}

function auto_sure(){
	var btype = '0';
	var data = {ajax_post:1};
	data.bid = '2454';
	if(btype == 0) data.type = 2;
	if(btype == 1) data.type = 1;
	data.money = $('#detail_money2').val();
	data.ticket = 0;
	if($("#xsb_ticket")[0] != undefined && $("#xsb_ticket")[0].checked == true) data.ticket = 1;
	$.ajax({
		url:'/member/auto/set_auto',
		data:data,
		dataType:'json',
		type:'post',
		success:function(d){
			$('.close-reveal-modal').click();
			if(d.status==1){
				$.jBox.tip(d.message,'success');
			}else{
				$.jBox.tip(d.message,'error');
			}
		}
	})
}
countdown();
</script>