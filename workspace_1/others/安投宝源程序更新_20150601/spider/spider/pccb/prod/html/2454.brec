var ajax_list = new Array();//用于存储 ajax 请求队列
/**
 * 查看预计回款计划
 */
function show_tender_detail(bid)
{
	try{
        ajax_list[ajax_list.length-1].abort();  //每次执行ajax前，终止前一个请求
    }catch(e){
    }
	$("#invest_sure").unbind();

	var unit = $("#unit_"+bid).val();
	var money = parseInt($("#result_"+bid).val());
	var base = parseInt($("#money_default_"+bid).val());
	if(isNaN(money)){
		$("#invest_title").html('PCCB投资确认');
		$("#invest_notice_bg").attr('class','reveal-green reveal-yellow clearfix');
		$("#invest_notice").html("您好，请输入正确的金额。");
		$(".reveal-list").hide();
		$(".reveal-border").hide();
		$("#invest_sure").hide();
		$("#invest_back").show();
		$('#myModal').reveal();
		return false;
	}
    if (money < base) {
		$("#invest_title").html('PCCB投资确认');
		$("#invest_notice_bg").attr('class','reveal-green reveal-yellow clearfix');
		$("#invest_notice").html("您好，购买本产品的最低限额为："+base+" "+unit+"元");
		$(".reveal-list").hide();
		$(".reveal-border").hide();
		$("#invest_back").show();
		$("#invest_sure").hide();
		$('#myModal').reveal();
        return false;
    }
    $("#invest_back").hide();
	$("#invest_sure").show();
	$(".reveal-list").show();
	$(".reveal-border").show();
	ajax_list[ajax_list.length] = $.ajax({
        url: cpurl+"/show_tender_detail",
        type: "post",
        dataType: "json",
        data: { bid:bid,unit:unit,money:money },
        success: function(d) {
			if(d.status == 0) return false;
			$(".reveal-table").html(d.html);
			$("#invest_title").html('回款计划');
			$("#invest_back").show();
			$("#invest_sure").hide();
			$(".reveal-con1").hide();
			$(".reveal-con2").show();
