<script type="text/javascript">

function submitNoticeForm(handler){
	if(handler == 0){
		//用户点击取消按钮；关闭弹出框并清除已经输入的数据
		$("#xm_tz").css("display","none");
	}else{
		var user_phone = $("#user_phone").val();
		var user_email = $("#user_email").val();
		if(user_email == '' || user_email == null || typeof(user_email) == 'undefined'){
			user_email = '';
		}else{
			 var regex = /^([0-9A-Za-z\-_\.]+)@([0-9a-z]+\.[a-z]{2,3}(\.[a-z]{2})?)$/g;
			 if (!regex.test(user_email)){
			 	alert("请输入正确的电子邮件地址");
			 	return false;
			 }
		}
		var formData = {'user_phone':user_phone,'user_email':user_email,'borrowers_id':'2a298a42c0f245c5912f2613c6f828aa'};
		//提交到后台处理
		$.ajax({
  			type: 'POST',
  			url: '/openNotice.do',
  			data: formData,
  			dataType: 'text',
  			success: function(result){
  				if(result == 'SUCCESS'){
  					alert("请求已受理，我们会在标的启投时使用手机短信和/或电子邮件的形式通知你！");
  				}else{
  					alert("系统繁忙，请稍后再试！");
  				}
  				$("#xm_tz").css("display","none");
  				$("#pop_box").css("display","none");
  			}
		});
	}
}

function closePop_box(){
		document.getElementById('pop_box').style.display='none';
		document.getElementById('rz_error').style.display='none';
	} 
function closeDiv()
{
	$("#pop_box").hide();
	$("#rz_error").hide();
	window.location.href = "/toRecharge.do?type=0";
}
function closeSuccess(){
	$("#pop_box").hide();
	$(".zq_tc_success").hide();
	window.location.reload();
}

function notice(){
	if('false' == 'true'){
		$("#pop_box").show();
		$("#xm_tz").show();
		return;
	}else{
		window.location.href = "/ucenter/new_login.jsp";
		return;
	}
}
$(function(){
	$(".redpakcet_c p").css("border-top","1px solid #ccc");
	
})
</script>
