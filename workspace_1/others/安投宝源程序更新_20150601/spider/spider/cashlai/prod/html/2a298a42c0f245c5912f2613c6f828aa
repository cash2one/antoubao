         $(function(){$("#toinvest").click(function(){
       		//$("#toinvest").attr("onclick","");
         	var param = {};
         	var investMoenys = $("#investMoneys").val();
         	var userBalance = '';
         	var m_val=investMoenys%10;	
         	var idValue = $("input[name='redPacketRadio']:checked").val();
         	if(idValue != null && idValue != ""){
         		$("#idAndValue").val(idValue);
         	}
         	if(investMoenys == "投资金额必须为10的倍数"){
        			$("#error_tip001").show().html("请输入投资金额");
        			//$("#toinvest").attr("onclick","investbid()");
        			return false;
        		}
         	if(m_val != 0){
           		$("#error_tip001").show().html("投资金额必须为10的倍数");
         		
         		$("#toinvest").attr("onclick","investbid()");
         		return false;
          		}
         	$("#error_tip001").hide().html("");
         	if($("#protocol").is(":checked") == false){
           	
           		$("#error_xy").show().html("投资之前必须同意《借款协议》");
           		//$("#toinvest").attr("onclick","investbid()");
           		return false;
           	}
         	if(investMoenys*10000 > userBalance*10000){
         		window.location.href= "/go_gopayPayment.do";
         		return false;
         	}
         	$(".error_tip00").hide().html("");
         	var investMoeny = investMoenys*100;
         	var minValue = $("#minValue").val()*100;
           	var maxValue = $("#maxValue").val()*100;
           	if(minValue == 0 || minValue == ''){
         		if(investMoeny < 1000 || investMoeny == ''){
         			alert("起投金额不能低于10元");
         			//$("#toinvest").attr("onclick","investbid()");
         			return false;
         		}
         	}
           	
           	else{
         		var decimalLentg = investMoenys.replace(/(\d*\.?)/,"").length;
         		if(decimalLentg > 2){
         			alert("起投金额最多精确至小数点后2位!");
         			//$("#toinvest").attr("onclick","investbid()");
         			return false;
         		}
         		
         		if(minValue < 1000){
         			if(investMoeny < minValue){
          			alert("起投金额不能低于10元");
          			//$("#toinvest").attr("onclick","investbid()");
          			return false;
          		}
         		}else{
         			if(investMoeny < minValue){
          			alert("起投金额不能低于" + minValue/100.00);
          			//$("#toinvest").attr("onclick","investbid()");
          			return false;
          		}
         		}
         	}
           	if(maxValue != 0 && maxValue != ''){
           		if(investMoeny > maxValue){
           			alert("起投金额不能高于" + maxValue/100.00);
           			//$("#toinvest").attr("onclick","investbid()");
          			return false;
           		}
           	}
           	
