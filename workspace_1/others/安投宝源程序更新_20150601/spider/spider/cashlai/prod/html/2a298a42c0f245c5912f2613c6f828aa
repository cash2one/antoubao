<script type="text/javascript">

function redpacket(url,ts,type){
       		   clearTimeout(ts);
          		timer = setTimeout(function(){
     					var investAmount = $(".alp_poi_ip").val();
     					var projectType = $(type).val();
     					$.ajax({
     						url : url,
     						type : "post",
     						async : false,
     						data : {'amount' : investAmount,'type':projectType},
         					dataType:'json',
         					success: function(json){
         						$("#redpakcet").html("<div id=\"redpakcet\"></div>");
         						var index=1;
         						for (var i=0,l=json.length;i<l;i++) {
         							var packetName = null;
         							var packetId = null;
         							var denomination = 0;
         							var lowLimit = 0;
         							var rewardType = null;
         							var singleLimit = null;
         							var singleLimitRate = null;
         							var useMoney = 0;
         							var optional=null;
									for(var key in json[i]){
										if(key == 'id'){packetId = json[i][key]};
										if(key == 'name'){packetName = json[i][key]};
										if(key == 'rewardType'){rewardType = json[i][key]};
										if(key == 'denomination'){denomination = json[i][key]};
										if(key == 'singleLimit'){singleLimit = json[i][key]};
										if(key == 'singleLimitRate'){singleLimitRate = json[i][key]};
										if(key == 'lowLimit'){lowLimit = json[i][key]};
										if(key == 'optional'){optional = json[i][key]}
									}
									if(rewardType == 0){
										useMoney = denomination;
									}else if(rewardType == 1){
										useMoney = singleLimitRate * investAmount * 0.01;
										if(useMoney > singleLimit){
											useMoney = singleLimit;
										}
									}else if(rewardType == 2){
										useMoney = singleLimitRate * investAmount * 0.01;
									}
									useMoney = useMoney.toFixed(2);
									
									$("#redpakcet").append(function(){
									var str;
									if(optional=='yes'){
										if(url=="redPacketToUse.do"&&index=="1"){ 
											 str = "<p class=\"poeijsbx_02\"><input type=\"radio\" id=\"redPacketRadio\" checked  name=\"redPacketRadio\" value=\""
											+ packetId + "," + useMoney + "\"></input>可使用"+packetName+ "<span class=\"c_ED7A55\">,返"
											+ useMoney + "</span>元";
											index="2";
										}else
										 str = "<p class=\"poeijsbx_02\"><input type=\"radio\" id=\"redPacketRadio\"  name=\"redPacketRadio\" value=\""
											+ packetId + "," + useMoney + "\"></input>可使用"+packetName+ "<span class=\"c_ED7A55\">,返"
											+ useMoney + "</span>元";
									}else{
										 str = "<p class=\"poeijsbx_02\"><input type=\"radio\" id=\"redPacketRadio\" disabled name=\"redPacketRadio\" value=\""
										+ packetId + "," + useMoney + "\"></input>可使用"+packetName+ "<span class=\"c_ED7A55\">,返"
										+ useMoney + "</span>元";
									}
									
										
										if(singleLimit != null && singleLimit != -1){
											str += "(最高<span class=\"c_ED7A55\">" + singleLimit + "</span>元)";
										}
										if(lowLimit != 0){
											str += "(使用门槛<span class=\"c_ED7A55\">￥" + lowLimit + "</span>)";
										}
										str += "</p>";
										return str;
								});
									
									// 项目详情
									var left_hei_03 = $(".project_list_left ").height();
									var right_hei_03 = $(".project_list_right").height();
									var pieo_rgiht_box=$(".pieo_rgiht_box").height();
									
										$(".project_list_right").height(left_hei_03);
									var proj_ri_next=$(".project_list_right").height()-pieo_rgiht_box-14
										$(".proj_ri_next").height(proj_ri_next)
								
							}
       					}
   					}); 
   				},500);
	}


