<script>
var  addtime =  '';
var  sales_prom_type =  0;
var  first_tender_amt =  '';
var  first_tender_time =  '';

		var logintype='user';
		var user_id = $('#login_user_id').val();
		$(document).ready(function () {
		    $("#c_head_r").click(function () {
		        $("#login_bg_box").hide();
		    });
		})
		function checkLogin(jumpurl){
			 if(jumpurl == 'speed'){
				 $('#backurl').val('/index.php?olap&q=sapply');
			 }else if(jumpurl == 'Payroll'){
				 $('#backurl').val('/index.php?olap&q=papply');
			 }
			if(user_id == ''){
				$("#login_bg_box").show();
			}else{
				 var backurl = $('#backurl').val();
				 	 backurl = backurl+"?detail=true";
				// var flag = is_new_user(addtime,sales_prom_type);
				 var flag = is_first_tender(first_tender_amt,first_tender_time,sales_prom_type);
				
				 if(!flag){
					 alert("您已经不是新用户，不能投新手标");
				 }else{
		 			 window.location.href = decodeURIComponent(backurl);
				 }
			}
		}	
	     /**
	     *个人登录校验
	     */
	     function check_user_form(){
	    	 var keywords = $("#keywords").val(); 
	    	 var password = $("#password").val(); 
	    	 if($.trim(keywords) == ''){
	    		 $("#user_msg_notice").html('请输入用户名/邮箱/手机号'); 
	    		 return false;
	    	 } else {
	    		 $("#user_msg_notice").html(''); 
	    	 }
			if($.trim(password) == ''){
			 $("#user_msg_notice").html('请输入密码'); 
	    		 return false;
	    	 }else{
	    		 $("#user_msg_notice").html('');
	    	 }
		}
       
       function loginSubmit(){
     	  
      	  var keywords = $('#keywords').val();
      	  var password = $('#password').val();
      	  var valicode = $('#captcha').val();
      	  if(keywords==''){
      	    	$('#user_msg_notice').html('请输入用户名/邮箱/手机号');
      	    	return;
      	 }else if(password==''){
      	    	$('#user_msg_notice').html('请输入登陆密码');
      	    	return;
      	 } 
      	 if($("#yanzheng").is(":visible")){
      		 v = "&valicode="+valicode;
      	 }else{
      		 v = "";
      	 }
      	 $.ajax({
      		   type: "POST",
      		   url: "/index.php?m&q=user/action_login",
      		   data: "&keywords="+keywords+"&password="+password+v ,
      		   dataType:"json",
      		   async: false,
      		   success: function(json){
      			 var code = json.code;
      			 if (code =='100000'){  
      			 	var backurl = $('#backurl').val();
      			 	if(backurl == ''){
      			 		backurl = '/index.php?user';
      			 	}
      		 	    window.location.href = decodeURIComponent(backurl);
      				$('#login_bg_box').hide();
      			 } else{
      				 if(code =='100002'){
      					 $('#password').val('');
      				 }else{
      					 $('#keywords').val('');
      					 $('#password').val('');
      				 }
      				 
      				 var password = json.msg.replace(/[^0-9]/ig, ""); 
     					 var num=parseInt(password); 
     					 if(num < 3){
     						 $('#yanzheng').show();
     					 }
      				 $('#user_msg_notice').html(json.msg);
      				 return;
      			 }
      			 
      		   },
      		   error:function(json){
      			   $('#user_msg_notice').html(json.msg);
      			   return false;
      		   }
      	});	
       }
</script>
 
